{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models",
      "../node_modules/loopback-component-passport/lib/models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../node_modules/loopback-setup-remote-methods-mixin"
    ]
  },
  "MyUser": {
    "dataSource": "accounts",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "__create__credentials": false,
          "__get__credentials": false,
          "__delete__credentials": false,
          "__destroyById__credentials": false,
          "__updateById__credentials": false,
          "__findById__credentials": false
        }
      }
    }
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "MyUser",
        "foreignKey": "userId"
      }
    }
  },
  "ApplicationCredential": {
    "dataSource": "db",
    "public": false
  },
  "UserCredential": {
    "dataSource": "db",
    "public": false
  },
  "MyUserIdentity": {
    "dataSource": "identities",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true
        }
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "rolemappings",
    "public": true
  },
  "Role": {
    "dataSource": "roles",
    "public": true
  },
  "Container": {
    "dataSource": "containers",
