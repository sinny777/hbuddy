{
  "initial:before": {
	  "loopback#favicon": {
	    	"params": "$!../client/dist/favicon.ico"
	    }
  },
  "initial": {
	    "compression": {},
	    "loopback-context#per-request": {},
	    "cors": {
	      "params": {
	        "origin": true,
	        "credentials": true,
	        "maxAge": 86400
	      }
	    }
	  },
  "files": {
    "loopback#static": {
      "params": "$!../client/dist"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "loopback#errorHandler": {
    	"params": {
            "includeStack": false
          }
    }
  }
}
