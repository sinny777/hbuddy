{
  "initial:before": {
    "loopback#favicon": {
      "params": "$!../client/dist/favicon.ico"
    }
  },
  "initial": {
    "loopback-context#per-request": {
      "params": {
        "enableHttpContext": true
      }
    },
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": [
      {
        "paths": "/",
        "params": "$!../client/dist"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
        "debug": true,
        "log": true
      }
    }
  }
}
